{
  "name": "@eevee/eevee-content",
  "version": "1.0.0",
  "description": "content for my personal blog",
  "private": true,
  "repository": {
    "type": "git",
    "url": "https://github.com/lexuantien/eevee-content"
  },
  "license": "MIT",
  "workspaces": {
    "packages": [
      "scripts",
      "typings"
    ],
    "nohoist": []
  },
  "scripts": {
    "create-post": "plop --plopfile ./scripts/create-files/post/plopfile.ts --dest . --require ./scripts/ts-node-register",
    "refresh-content": "node --experimental-specifier-resolution=node --loader ts-node/esm scripts/github/getChangedFiles.ts"
  },
  "main": "index.js",
  "devDependencies": {
    "@code-hike/mdx": "0.6.1",
    "@types/lerna-alias": "^3.0.1",
    "@types/node": "^17.0.36",
    "@types/node-fetch": "^2.6.1",
    "@types/react": "^18.0.14",
    "@types/react-dom": "17.0.11",
    "@types/yargs": "^17.0.10",
    "@types/yargs-unparser": "^2.0.1",
    "big-integer": "1.6.51",
    "cross-env": "^7.0.3",
    "esbuild-loader": "^2.19.0",
    "firebase": "9.8.3",
    "jju": "^1.4.0",
    "lerna-alias": "^3.0.2",
    "long": "5.2.0",
    "mdx-bundler": "9.0.1",
    "node-plop": "^0.25.0",
    "p-queue": "^6.0.2",
    "plop": "^2.6.0",
    "react": "^18.2.0",
    "react-test-renderer": "^18.1.0",
    "reading-time": "1.5.0",
    "remark-mdx-images": "1.0.3",
    "semver": "^6.2.0",
    "syncpack": "5.6.10",
    "tmp": "^0.2.1",
    "ts-loader": "^9.3.0",
    "ts-node": "^10.8.0",
    "tslib": "2.4.0",
    "typescript": "4.3.5",
    "yargs": "^17.5.1",
    "yargs-parser": "13.1.2",
    "yargs-unparser": "^2.0.0"
  },
  "syncpack": {
    "prod": true,
    "dev": true,
    "peer": false,
    "source": [
      "package.json",
      "scripts/package.json"
    ],
    "versionGroups": [
      {
        "packages": [
          "@eevee/eevee-repo"
        ],
        "dependencies": [
          "chalk",
          "fs-extra",
          "glob",
          "jju",
          "yargs",
          "tslib"
        ]
      }
    ]
  }
}
